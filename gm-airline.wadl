<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://wadl.dev.java.net/2009/02" xmlns:tns="default/schemas">
  <doc title="Global Miles Airline API" xml:lang="en">API for Airlines</doc>
  <grammars>
    <xs:schema xmlns:tns="default/schemas" targetNamespace="default/schemas" xmlns:xs="http://www.w3.org/2001/XMLSchema">
      <xs:element name="FlightCreateRequest" type="tns:FlightCreateRequest" />
      <xs:element name="Response" type="tns:Response" />
      <xs:element name="Amount" type="tns:Amount" />
      <xs:element name="FlightMax" type="tns:FlightMax" />
      <xs:element name="GenderType" type="tns:GenderType" />
      <xs:element name="PassengerMax" type="tns:PassengerMax" />
      <xs:element name="AwardMilesRules" type="tns:AwardMilesRules" />
      <xs:element name="LanguageType" type="tns:LanguageType" />
      <xs:element name="AwardType" type="tns:AwardType" />
      <xs:element name="MemberSearchRequest" type="tns:MemberSearchRequest" />
      <xs:element name="FlightStatusType" type="tns:FlightStatusType" />
      <xs:element name="PassengerMin" type="tns:PassengerMin" />
      <xs:element name="NewMemberRequest" type="tns:NewMemberRequest" />
      <xs:element name="FlightMin" type="tns:FlightMin" />
      <xs:element name="MemberSearchResponse" type="tns:MemberSearchResponse" />
      <xs:element name="FlightSimple" type="tns:FlightSimple" />
      <xs:element name="User" type="tns:User" />
      <xs:element name="FlightStatusResponse" type="tns:FlightStatusResponse" />
      <xs:element name="FlightStatusRequest" type="tns:FlightStatusRequest" />
      <xs:element name="NewMemberResponse" type="tns:NewMemberResponse" />
      <xs:element name="ExtraInfoType" type="tns:ExtraInfoType" />
      <xs:element name="RoutingType" type="tns:RoutingType" />
      <xs:element name="TierType" type="tns:TierType" />
      <xs:element name="CabinType" type="tns:CabinType" />
      <xs:complexType name="FlightCreateRequest">
        <xs:sequence>
          <xs:element minOccurs="1" name="award_miles_rules" type="tns:AwardMilesRules">
            <xs:annotation>
              <xs:documentation>A complex object for definition of the award mile calculation logic.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="passenger" type="tns:PassengerMax">
            <xs:annotation>
              <xs:documentation>A complex object for representing a passenger.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="routing_type" type="tns:RoutingType">
            <xs:annotation>
              <xs:documentation>Routing type of the flights.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" maxOccurs="unbounded" name="flights" type="tns:FlightMax">
            <xs:annotation>
              <xs:documentation>An array of flights. We are expecting one flight for one way trip and two flights for round trip.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Response">
        <xs:annotation>
          <xs:documentation>Basic response object</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="status">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The status of the request. It is always 'ok' for successful responses.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="message">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The message for the unsuccessful request. It is always null for successful responses.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="Amount">
        <xs:annotation>
          <xs:documentation>A complex object for the amount.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="currency_code">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO 4217 currency code of the amount.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="value">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>A decimal value with two decimals in the string representation of amount.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:double" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="FlightMax">
        <xs:sequence>
          <xs:element minOccurs="1" name="airline">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for airline company.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="cabin_type">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Cabin type of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="flight_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Number of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="pnr">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Passenger name record of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="ticket_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Ticket number of the passenger.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="departure_airport">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for the departure airport.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="arrival_airport">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for the  arrival airport.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="departure_date_time">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO 8601 date and time in UTC for departure time.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="arrival_date_time">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO 8601 date and time in UTC for arrival time.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="total_amount" type="tns:Amount">
            <xs:annotation>
              <xs:documentation>A complex object for the amount of the flight including currency and value.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="distance">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The distance between departure and arrival airports.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="GenderType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="male">
            <xs:annotation>
              <xs:documentation>Male</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="female">
            <xs:annotation>
              <xs:documentation>Female</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="unknown">
            <xs:annotation>
              <xs:documentation>Unknown</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="PassengerMax">
        <xs:annotation>
          <xs:documentation>A complex object for representing a passenger.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="first_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>First name of the passenger. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="last_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Last name of the passenger. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="mobile">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Mobile phone number of the passenger. It is a unique identifier for passengers in the system. The format is E.164.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="email">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Email address of the passenger.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" default="en" name="language" type="tns:LanguageType">
            <xs:annotation>
              <xs:documentation>The language of the passenger. Possible values are 'en' and 'tr'. Default value is 'en'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="gender" type="tns:GenderType">
            <xs:annotation>
              <xs:documentation>Gender of the passenger. The possible values are 'female', 'male' or 'unknown'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="birthday">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Birthday of the passenger. The possible values are a date with YYYYMMDD formatted or null.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="AwardMilesRules">
        <xs:annotation>
          <xs:documentation>A complex object for definition of the award mile calculation logic.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="award_type" type="tns:AwardType">
            <xs:annotation>
              <xs:documentation>The type of the award base. The possible values are 'distance_based' and 'fare_based'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="base_ratio">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The ratio for calculating base mile from distance or fare. Possible value is an integer between 1 and 100.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="checkin_ratio">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The additional mile ratio for a check-in operation. Possible value as an integer between 0 and 100.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="checkout_ratio">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The additional mile ratio for a check-out operation. Possible value as an integer between 0 and 100.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="survey_ratio">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The additional mile ratio for a survey. Possible value as an integer between 0 and 100.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:integer" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="LanguageType">
        <xs:restriction base="xs:string">
          <xs:enumeration value="en">
            <xs:annotation>
              <xs:documentation>for English</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="tr">
            <xs:annotation>
              <xs:documentation>for Turkish</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType name="AwardType">
        <xs:annotation>
          <xs:documentation>The type of the award base. The possible values are 'distance_based' and 'fare_based'.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="distance_based" />
          <xs:enumeration value="fare_based" />
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="MemberSearchRequest">
        <xs:sequence>
          <xs:element minOccurs="1" name="ffp_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>FFP number of member</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="first_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>First name</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="last_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Last name</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:simpleType name="FlightStatusType">
        <xs:annotation>
          <xs:documentation>The final status of flight. The possible values are 'used', 'unused' and 'reissued'.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
          <xs:enumeration value="used">
            <xs:annotation>
              <xs:documentation>Used</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="unused">
            <xs:annotation>
              <xs:documentation>Unused</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="reissued">
            <xs:annotation>
              <xs:documentation>Reissued</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
      <xs:complexType name="PassengerMin">
        <xs:annotation>
          <xs:documentation>A complex object for representing a minimal passenger information.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="first_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>First name of the passenger. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="last_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Last name of the passenger. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="mobile">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Mobile phone number of the passenger. It is a unique identifier for passengers in the system. The format is E.164.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="email">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Email address of the passenger.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="NewMemberRequest">
        <xs:sequence>
          <xs:element minOccurs="1" name="user" type="tns:User">
            <xs:annotation>
              <xs:documentation>A complex object for representing a user.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="flight" type="tns:FlightSimple">
            <xs:annotation>
              <xs:documentation>A complex object for flight.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="FlightMin">
        <xs:sequence>
          <xs:element minOccurs="1" name="pnr">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Passenger name record of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="ticket_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Ticket number of the passenger.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="flight_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Number of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="MemberSearchResponse">
        <xs:complexContent mixed="false">
          <xs:extension base="tns:Response">
            <xs:sequence>
              <xs:element minOccurs="1" name="tier_type" type="tns:TierType">
                <xs:annotation>
                  <xs:documentation>Type of tiers.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="1" name="base_miles">
                <xs:simpleType>
                  <xs:annotation>
                    <xs:documentation>Value of current miles.</xs:documentation>
                  </xs:annotation>
                  <xs:restriction base="xs:integer" />
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="1" name="promo_miles">
                <xs:simpleType>
                  <xs:annotation>
                    <xs:documentation>Value of status miles.</xs:documentation>
                  </xs:annotation>
                  <xs:restriction base="xs:integer" />
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:extension>
        </xs:complexContent>
      </xs:complexType>
      <xs:complexType name="FlightSimple">
        <xs:annotation>
          <xs:documentation>A complex object for flight.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="airline">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for airline company.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="pnr">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Passenger name record of the flight.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="departure_date">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>ISO 8601 date for departure time.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="departure_airport">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for the departure airport.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="arrival_airport">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>IATA code for the  arrival airport.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="User">
        <xs:annotation>
          <xs:documentation>A complex object for representing a user.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element minOccurs="1" name="first_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>First name of the user. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="last_name">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Last name of the user. Maximum length is 40 characters.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="mobile">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Mobile phone number of the user It is a unique identifier for users in the system. The format is E.164.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="email">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Email address of the user.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="language" type="tns:LanguageType">
            <xs:annotation>
              <xs:documentation>The language of the user. Possible values are 'en' and 'tr'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="gender" type="tns:GenderType">
            <xs:annotation>
              <xs:documentation>Gender of the user. The possible values are 'female', 'male' or 'unknown'.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="birthday">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Birthday of the user. The possible values are a date with YYYYMMDD formatted or null.</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="nationality">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Nationality</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="national_id_number">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>National ID Number</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="academic_title">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Academic Title</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="address">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Address</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="country">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>Country</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string" />
            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="0" name="city">
            <xs:simpleType>
